CREATE OR REPLACE VIEW iv_product_battery_charger_codes FOLDER = '/2 - integration' AS SELECT bv_wo_stock.distribution_code AS distribution_code, bv_product.productname AS product_name, bv_wo_storages.city AS city, count(*) AS amount FROM (bv_product AS bv_product INNER JOIN bv_wo_stock AS bv_wo_stock ON bv_product.productid = bv_wo_stock.product_id ) INNER JOIN bv_wo_storages AS bv_wo_storages ON bv_wo_stock.storage_id = bv_wo_storages.id  GROUP BY bv_product.productid, bv_product.productname, bv_wo_storages.id, bv_wo_storages.city, bv_wo_stock.distribution_code HAVING bv_product.productname like '%Battery Charger%';

